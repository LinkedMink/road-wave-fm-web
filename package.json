{
  "name": "@linkedmink/road-wave-fm-web",
  "version": "2.0.0-1",
  "description": "A web application to communicate with the Road Wave FM data source",
  "homepage": "https://github.com/LinkedMink/road-wave-fm-web#readme",
  "bugs": {
    "url": "https://github.com/LinkedMink/road-wave-fm-web/issues",
    "email": "harlan.sang@linkedmink.net"
  },
  "license": "MIT",
  "author": {
    "name": "Harlan Sang",
    "email": "harlan.sang@linkedmink.net",
    "url": "https://github.com/LinkedMink"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/LinkedMink"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LinkedMink/road-wave-fm-web.git"
  },
  "type": "module",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start": "dotenvx run --env TARGET_ENV=local -- webpack serve --open --config config/webpack.serve.ts",
    "start:optimized": "docker compose up -d -- web && dotenvx run --env TARGET_ENV=local -- webpack --watch --config config/webpack.prod.ts",
    "build": "dotenvx run --env TARGET_ENV=production -- webpack --config config/webpack.prod.ts",
    "build:dev": "dotenvx run --env TARGET_ENV=development -- webpack --config config/webpack.dev.ts",
    "build:local": "dotenvx run --env TARGET_ENV=local -- webpack --config config/webpack.local.ts",
    "build:analyze": "webpack-bundle-analyzer dist/stats.json",
    "build:docker": "dotenvx run -- docker compose build --pull -- web",
    "test": "jest --config config/jest.config.js --watch",
    "test:ci": "jest --coverage --ci --config config/jest.config.js --passWithNoTests",
    "test:debug": "jest --cache=false --detectOpenHandles --runInBand --config config/jest.config.js --watch",
    "lint": "eslint src/**/*.ts{,x} config/**/*.ts *.js",
    "format": "prettier --write src/**/*.ts{,x} config/**/*.ts *.js",
    "preversion": "npm run lint",
    "version": "npm run build && git add .",
    "postversion": "npm run build:docker",
    "prepare": "husky || cd ."
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@fontsource/roboto": "^5.0.8",
    "@linkedmink/eip-4361-parser": "^1.0.0-16",
    "ethers": "^6.13.2",
    "@mui/icons-material": "^6.0.1",
    "@mui/material": "^6.0.1",
    "@tanstack/react-query": "^5.51.23",
    "@trpc/client": "^11.0.0-rc.477",
    "@trpc/react-query": "^11.0.0-rc.477",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-use": "^17.5.0",
    "web-vitals": "^4.2.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@dotenvx/dotenvx": "^1.6.4",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.1",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/google.maps": "^3.50.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/webpack-bundle-analyzer": "^4.6.3",
    "babel-jest": "^29.7.0",
    "browserslist": "^4.21.3",
    "compression-webpack-plugin": "^11.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.0.0",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^9.8.0",
    "eslint-plugin-react": "^7.31.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "globals": "^15.8.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.1",
    "markdown-loader": "^8.0.0",
    "mini-css-extract-plugin": "^2.8.0",
    "postcss": "^8.4.33",
    "postcss-loader": "^8.1.0",
    "postcss-preset-env": "^10.0.0",
    "prettier": "^3.2.4",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "tsx": "^4.7.1",
    "type-fest": "^4.10.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.0",
    "webpack": "^5.90.1",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.1",
    "webpack-merge": "^6.0.1"
  }
}
