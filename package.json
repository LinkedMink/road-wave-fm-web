{
  "name": "@linkedmink/road-wave-fm-web",
  "version": "1.3.0-0",
  "description": "A web application to communicate with the Road Wave FM data source",
  "homepage": "https://github.com/LinkedMink/road-wave-fm-web#readme",
  "bugs": {
    "url": "https://github.com/LinkedMink/road-wave-fm-web/issues",
    "email": "harlan.sang@linkedmink.net"
  },
  "license": "MIT",
  "author": {
    "name": "Harlan Sang",
    "email": "harlan.sang@linkedmink.net",
    "url": "https://github.com/LinkedMink"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/LinkedMink"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LinkedMink/road-wave-fm-web.git"
  },
  "type": "module",
  "private": true,
  "scripts": {
    "start": "cross-env-shell NODE_OPTIONS=\"--import tsx\" TARGET_ENV=local-dev webpack serve --open --config config/webpack.serve.ts",
    "start:optimized": "docker-compose up -d && cross-env-shell NODE_OPTIONS=\"--import tsx\" TARGET_ENV=local-optimized webpack --watch --config config/webpack.prod.ts",
    "build": "cross-env-shell NODE_OPTIONS=\"--import tsx\" TARGET_ENV=production webpack --config config/webpack.prod.ts",
    "build:dev": "cross-env-shell NODE_OPTIONS=\"--import tsx\" TARGET_ENV=development webpack --config config/webpack.dev.ts",
    "build:local": "cross-env-shell NODE_OPTIONS=\"--import tsx\" TARGET_ENV=localhost webpack --config config/webpack.local.ts",
    "build:analyze": "webpack-bundle-analyzer dist/stats.json",
    "test": "jest --watchAll --config config/jest.config.mjs",
    "test:ci": "jest --coverage --ci --config config/jest.config.mjs --passWithNoTests",
    "test:debug": "jest --cache=false --detectOpenHandles --runInBand --watchAll --config config/jest.config.mjs",
    "lint": "eslint src/**/*.ts{,x} config/**/*.{,m,c}ts *.{,m,c}js",
    "format": "prettier --write src/**/*.ts{,x} deploy/**/*.{,m,c}ts *.{m,c,}js{,on}",
    "preversion": "npm run lint",
    "version": "npm run build && git add .",
    "postversion": "bash ./docker/build.sh",
    "prepare": "husky || cd ."
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.10.2",
    "@mui/material": "^5.10.2",
    "jose": "^5.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-use": "^17.5.0",
    "web-vitals": "^3.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.1",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.4.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/google.maps": "^3.50.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/webpack-bundle-analyzer": "^4.6.3",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "babel-jest": "^29.7.0",
    "browserslist": "^4.21.3",
    "compression-webpack-plugin": "^11.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.0.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.1",
    "markdown-loader": "^8.0.0",
    "mini-css-extract-plugin": "^2.8.0",
    "postcss": "^8.4.33",
    "postcss-loader": "^8.1.0",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.2.4",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "tsx": "^4.7.1",
    "type-fest": "^4.10.2",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.1",
    "webpack-merge": "^5.10.0"
  }
}
